    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/fs-extra": "^4.0.5",
    "@types/glob": "^5.0.33",
    "@types/inquirer": "0.0.35",
    "@types/mocha": "^2.2.44",
