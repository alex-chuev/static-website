{
  "name": "static-website",
  "version": "1.0.0",
  "description": "Create a static multilingual website",
  "repository": "https://github.com/alex-chuev/static-website.git",
  "main": "dist/index.js",
  "dependencies": {
    "commander": "^2.12.1",
    "fs-extra": "^4.0.2",
